{
  "lineWidth": 120,
  "indentWidth": 2,
  "useTabs": false,
  "typescript": {
    "semiColons": "always",
    "quoteStyle": "preferDouble",
    "trailingCommas": "always"
  },
  "json": {},
  "markdown": {},
  "excludes": [
    "node_modules",
    "dist",
    "src-tauri/target",
    "src-tauri/Cargo.lock",
    "vendor"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.93.3.wasm",
    "https://plugins.dprint.dev/json-0.19.4.wasm",
    "https://plugins.dprint.dev/markdown-0.17.8.wasm"
  ]
}
